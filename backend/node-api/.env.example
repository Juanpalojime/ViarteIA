# Node API Environment Variables
# Copy this to .env and fill with your actual values

# CRITICAL: Groq AI API Key
GROQ_API_KEY=your_groq_api_key_here

# Server Configuration
PORT=3001
NODE_ENV=development

# Database
DATABASE_URL=file:./dev.db

# Security
JWT_SECRET=your_jwt_secret_here_change_me_to_random_string

# External Services
PYTHON_API_URL=http://localhost:8000

# Optional: S3 Storage (if you plan to use AWS S3)
# S3_ACCESS_KEY=your_s3_access_key
# S3_SECRET_KEY=your_s3_secret_key
# S3_BUCKET=your_bucket_name
# S3_REGION=us-east-1
